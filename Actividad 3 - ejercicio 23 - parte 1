    private void btnImprimirActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
        
        double A, B, C, solucion0, solucion1; 
        String mensaje;
        

        A =  Double.parseDouble(txtValora.getText());
        B =  Double.parseDouble(txtValorb.getText());
        C =  Double.parseDouble(txtValorc.getText());
        
        solucion0 = (-B + Math.sqrt(Math.pow(B, 2)-4*A*C))/(2*A);
        solucion1 = (-B - Math.sqrt(Math.pow(B, 2)-4*A*C))/(2*A);
        mensaje = ("Sus soluciones son: " + solucion0 + " y " + solucion1);
        txtSoluciones.setText(String.valueOf(mensaje));
        
    }                                           

    private void btnBorrarActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        txtValorb.setText("");
        txtValora.setText("");
        txtValorc.setText("");
        txtSoluciones.setText("");
    }                                         

    private void btnSalirActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        dispose();
    }                                        

