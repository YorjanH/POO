    private void btnImprimirActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
        
        double numero1, numero2;
        String mensaje;
        

        numero1 =  Double.parseDouble(txtNumero1.getText());
        numero2 =  Double.parseDouble(txtNumero2.getText());
        
        if (numero1 > numero2){
            mensaje = (numero1 + " es mayor que " + numero2);
            txtMayor.setText(String.valueOf(mensaje));
        } else {
            if (numero1 == numero2){
                mensaje = (numero2 + " es igual que " + numero1);
                txtMayor.setText(String.valueOf(mensaje));
            } else {
                mensaje = (numero2 + " es mayor que " + numero1);
                txtMayor.setText(String.valueOf(mensaje));
            }
        }
  
        
        
    }                                           

    private void btnBorrarActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        txtNumero2.setText("");
        txtNumero1.setText("");
        txtMayor.setText("");
    }                                         

    private void btnSalirActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        dispose();
    }                                        

